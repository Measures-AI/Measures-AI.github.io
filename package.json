{
  "name": "react-measures",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "echo 'measuresai.com' > ./dist/CNAME && gh-pages -d dist",
    "build": "vite build && node scripts/generate-static-pages.js",
